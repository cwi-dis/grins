# Irix 5.X version, with support both for GL and X.
BASEDIR=	$(srcdir)/../python/Extensions/X11
BASELIB=	$(BASEDIR)/irix5/lib.a
BASESETUP=	$(BASEDIR)/irix5/Setup

mm mmmodule.o -DMM_DEBUG

moviechannel moviechannelmodule.o -DMM_DEBUG -DUSE_CL -DUSE_XM -DUSE_GL -I$(BASEDIR)
mpegchannel mpegchannelmodule.o -DMM_DEBUG -DUSE_CL -DUSE_XM -DUSE_GL -I$(BASEDIR)
soundchannel soundchannelmodule.o -DMM_DEBUG
